  <PropertyGroup Condition="'$(TargetFramework)' == 'net6.0'">
    <SystemVersion>6.0.*</SystemVersion>
    <ExtensionsVersion>6.0.*</ExtensionsVersion>
    <AspNetCoreVersion>6.0.*</AspNetCoreVersion>
    <JwtTokensVersion>6.20.*</JwtTokensVersion>
